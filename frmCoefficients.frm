Attribute VB_Name = "frmCoefficients"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Const nboX As Integer = 28

Private Sub Toolbar1_ButtonClick(ByVal Button As MSComctlLib.Button)

Select Case Button

Case "Undo"
     UpdateCoefValues (1)

Case "Cancel"
     
     Unload Me
      
Case "OK"
     Icalc = 0
     UpdateCoefValues (2)
     Unload Me

Case "Help"
     ShowHelp (HelpContextID)
        
Case "Defaults"
    If MsgBox("Assign Default Values for Each Coefficient?", vbYesNo) = vbYes Then UpdateCoefValues (3)
            
End Select
End Sub
Private Sub Form_Load()
    SetToolTips
    UpdateCoefValues (1)
End Sub
Private Sub UpdateCoefValues(io)
'io=1 copy source values to temporary array
'io=2 copy from temporary array to original
'io=3 copy defaults

Select Case io

Case 1   'fill temporary array
    
    For j = 1 To NXk
        Text1(j - 1) = Xk(j)
        If j < 13 Then Text1(j + 16) = CvXk(j)
        Next j

    
Case 2    'copy from temporary array back to original values
       
    For j = 1 To NXk
        Xk(j) = Text1(j - 1)
        If j < 13 Then CvXk(j) = Text1(j + 16)
        Next j
    

Case 3    'set defaults
    
    For j = 1 To NXk
        Text1(j - 1) = XkDefault(j)
        If j < 13 Then Text1(j + 16) = CvXkDefault(j)
        Next j

Case default

End Select

End Sub
